{
  "name": "express_graphql",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch": "concurrently \"tsc --watch\" \"graphql-code-generator --watch\"",
    "start1": "node dist/src/index.js",
    "dev": "npm run compile && nodemon dist/src/index.js",
    "generate": "graphql-code-generator --config ./codegen.ts",
    "compile": "npm run generate && tsc",
    "dev1": "ts-node src/index.ts --exit-child --transpile-only ",
    "typeorm": "ts-node --files -r tsconfig-paths/register ./node_modules/typeorm/cli.js --config src/ormconfig.ts",
    "migration:create": "typeorm migration:create \"./src/migrations/",
    "migration:generate": "npm run typeorm migration:generate \"./src/migrations/",
    "migration:blank": "npm run typeorm migration:create -n"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@apollo/server": "^4.2.2",
    "@mikro-orm/cli": "^5.5.3",
    "@mikro-orm/migrations": "^5.5.3",
    "argon2": "^0.30.2",
    "concurrently": "^7.6.0",
    "connect-redis": "^6.1.3",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "graphql": "^15.7.2",
    "passport": "^0.6.0",
    "passport-google-oauth": "^2.0.0",
    "passport-google-oauth20": "^2.0.0",
    "pg": "^8.8.0",
    "redis": "^4.5.1",
    "reflect-metadata": "^0.1.13",
    "type-graphql": "^1.1.1",
    "typeorm": "^0.3.11",
    "urql": "^3.0.3",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.16.1",
    "@graphql-codegen/typescript": "^2.8.5",
    "@graphql-codegen/typescript-operations": "^2.5.11",
    "@graphql-codegen/typescript-resolvers": "^2.7.10",
    "@graphql-codegen/typescript-urql": "^3.7.3",
    "@graphql-tools/webpack-loader": "^6.7.1",
    "@types/connect-redis": "^0.0.19",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.14",
    "@types/express-session": "^1.17.5",
    "@types/node": "^18.11.10",
    "@types/passport": "^1.0.11",
    "@types/passport-google-oauth": "^1.0.42",
    "@types/passport-google-oauth20": "^2.0.11",
    "@types/validator": "^13.7.10",
    "babel-plugin-import-graphql": "^2.8.1",
    "nodemon": "^2.0.20",
    "ts-node": "^10.9.1"
  }
}
